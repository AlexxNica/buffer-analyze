{
  "name": "@bufferapp/date-picker",
  "version": "0.0.1",
  "description": "Date picker for Insights",
  "main": "index.js",
  "scripts": {
    "start": "start-storybook -p 9001",
    "lint": "eslint . --ignore-pattern coverage node_modules",
    "test": "yarn run lint && sh ../../package_test.sh",
    "test-watch": "jest --watch",
    "test-update": "jest -u --config=../../.jestrc.json"
  },
  "jest": {
    "transformIgnorePatterns": [
      "node_modules/(?!@bufferapp/*)"
    ],
    "verbose": true,
    "moduleNameMapper": {
      "\\.(css|less)$": "identity-obj-proxy"
    }
  },
  "author": "mike@msanroman.io",
  "dependencies": {
    "moment": "^2.18.1",
    "react-tooltip": "^3.3.0",
    "@bufferapp/async-data-fetch": "0.5.25",
    "@bufferapp/analyze-profile-selector": "0.0.1"
  },
  "devDependencies": {
    "@storybook/addon-storyshots": "3.1.4",
    "@storybook/react": "3.1.3",
    "eslint": "3.19.0",
    "jest": "19.0.2",
    "storybook-addon-a11y": "3.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
